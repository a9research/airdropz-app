{
  "name": "airdropz",
  "version": "0.0.01",
  "description": "Electron app with Next.js and proxy browser functionality",
  "author": "Austin",
  "private": true,
  "main": "electron/main.js",
  "preload": "electron/preload.js",
  "scripts": {
    "dev": "concurrently \"next dev --turbopack\" \"wait-on http://localhost:3000 && electron-forge start\"",
    "build": "next build --turbopack && electron-forge package",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint",
    "electron:dev": "concurrently \"next dev -p 3000\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "next build --turbopack && electron-next build ./src"
  },
  "dependencies": {
    "crawlee": "^3.15.0",
    "electron-squirrel-startup": "^1.0.1",
    "next": "15.5.3",
    "playwright": "^1.55.0",
    "pouchdb": "^9.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-dmg": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pouchdb": "^6.4.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "electron": "38.1.2",
    "electron-next": "^3.1.5",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "pluggable-electron": "^1.0.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^9.0.0"
  }
}
